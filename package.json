{
    "name": "testd3",
    "displayName": "testd3",
    "description": "Trying out webview with d3",
    "version": "0.0.1",
    "publisher": "self",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.testD3js",
        "onCommand:extension.refactor"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.testD3js",
                "title": "test",
                "category": "Python Profiler"
            },
            {
                "command": "extension.refactor",
                "title": "refactor",
                "category": "Python Profiler"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.testD3js",
                    "title" : "Profile"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "css-loader": "^6.7.3",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.4.2",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "ag-grid-community": "^19.1.1",
        "c3": "^0.6.8",
        "d3": "^5.7.0",
        "jquery": "^3.3.1",
        "jquery-ui": "^1.12.1"
    }
}
